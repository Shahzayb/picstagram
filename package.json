{
  "name": "pistagram",
  "version": "1.0.0",
  "description": "A clone of instagram",
  "main": "app.js",
  "private": true,
  "scripts": {
    "client": "npm start --prefix client",
    "server": "nodemon app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest --runInBand"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/tests/util/",
      "/tests/config/",
      "/client/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shahzayb/pistagram.git"
  },
  "keywords": [],
  "author": "Shahzaib Sarwar <imshahzayb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shahzayb/pistagram/issues"
  },
  "homepage": "https://github.com/Shahzayb/pistagram#readme",
  "devDependencies": {
    "concurrently": "^5.0.2",
    "dotenv": "^8.2.0",
    "jest": "^25.1.0",
    "mongodb-memory-server": "^6.2.3",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-validator": "^6.3.1",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.4",
    "morgan": "^1.9.1"
  }
}
